
<h1><%= link_to "#{@artist.name}", "/artists/#{@artist.id}"%></h1>
<p><%= @artist.bio %></p>
<%= form_for @artist do |f| %>
  <ul>
    <li>
  <%= f.label :name %>
  <%= f.text_field :name %>
    </li>
    <li>
  <%= f.label :bio %>
  <%= f.text_field :bio %>
    </li>
    <li>
  <%= f.submit %>
    </li>
  </ul>
<% end %>
